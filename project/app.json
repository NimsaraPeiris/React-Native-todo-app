{
  "expo": {
    "name": "Task Flow",
    "slug": "task-flow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "name": "Task Flow",
      "bundler": "metro",
      "output": "single",
      "plugins": ["expo-router", "expo-font", "expo-web-browser"],
      "experiments": {
        "typedRoutes": true
      }
    }
  }
}
